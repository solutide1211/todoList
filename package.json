{
  "name": "todolist",
  "version": "1.0.0",
  "description": "An Electron application with Vue",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "dotenv -e .env -- electron-builder",
    "build:renderer": "electron-vite build",
    "build:preload": "electron-vite build --mode preload",
    "build:main": "electron-vite build --mode main",
    "build:all": "npm run build:renderer && npm run build:main",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@element-plus/icons-vue": "^2.3.2",
    "@faker-js/faker": "^9.9.0",
    "diff": "^8.0.2",
    "echarts": "^6.0.0",
    "element-plus": "^2.11.2",
    "highlight.js": "^11.11.1",
    "js-yaml": "^4.1.0",
    "vue-router": "^4.5.1",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.0.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "dotenv-cli": "^10.0.0",
    "electron": "^37.2.3",
    "electron-builder": "^25.1.8",
    "electron-vite": "^4.0.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "^10.3.0",
    "prettier": "^3.6.2",
    "vite": "^7.0.5",
    "vue": "^3.5.17",
    "vue-eslint-parser": "^10.2.0"
  }
}
